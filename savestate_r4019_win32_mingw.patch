+			case 7: vga.draw.font_tables[lcv] = &(vga.draw.font[56*1024]); break;
+		}
+	}
+
+
+	switch( drawline_idx ) {
+		case 1: VGA_DrawLine = VGA_Draw_1BPP_Line; break;
+		case 3: VGA_DrawLine = VGA_Draw_2BPP_Line; break;
+		case 4: VGA_DrawLine = VGA_Draw_2BPPHiRes_Line; break;

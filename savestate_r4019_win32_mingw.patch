+
+
+					if( &pic_queue.entries[lcv] == pic_queue.free_entry ) pic_free_idx = lcv;
+					if( &pic_queue.entries[lcv] == pic_queue.next_entry ) pic_next_idx = lcv;
+				}
+
+				// - reloc ptrs
